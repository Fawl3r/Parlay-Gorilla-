{
  "name": "f3-parlay-ai-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean:next": "node -e \"const fs=require('fs'); fs.rmSync('.next',{recursive:true,force:true}); fs.rmSync('.turbo',{recursive:true,force:true});\"",
    "predev": "npm run clean:next",
    "predev:network": "npm run clean:next",
    "dev": "next dev --webpack -p 3000",
    "dev:network": "next dev --webpack -H 0.0.0.0 -p 3000",
    "dev:turbopack": "next dev --turbopack -p 3000",
    "dev:network:turbopack": "next dev --turbopack -H 0.0.0.0 -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:server-bundle": "node scripts/verify-next-server-bundle.js",
    "test:smoke:build": "npm run clean:next && npm run build && npm run test:server-bundle",
    "test:unit": "vitest run",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "axios": "^1.6.5",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.562.0",
    "next": "^16.1.0",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.5.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^20.11.5",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.17",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.0",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vitest": "^2.1.9",
    "@playwright/test": "^1.45.0"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  }
}
